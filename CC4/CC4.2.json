{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Annual Average Air Quality Index (AQI) in Delhi, 2018-2023",
    "fontSize": 14,
    "fontWeight": "bold",
    "anchor": "start"
  },
  "width": 500,
  "height": 350,
  "data": {
    "values": [
      {"Year": "2018", "AQI": 225},
      {"Year": "2019", "AQI": 215},
      {"Year": "2020", "AQI": 185},
      {"Year": "2021", "AQI": 209},
      {"Year": "2022", "AQI": 209},
      {"Year": "2023", "AQI": 204}
    ]
  },
  "mark": {
    "type": "bar",
    "color": "#ded0f2",
    "cornerRadiusTopLeft": 4,
    "cornerRadiusTopRight": 4
  },
  "encoding": {
    "x": {
      "field": "Year",
      "type": "nominal",
      "axis": {"title": "Year", "labelFontSize": 11, "labelAngle": 0}
    },
    "y": {
      "field": "AQI",
      "type": "quantitative",
      "axis": {"title": "Average Daily AQI", "grid": true, "labelFontSize": 11},
      "scale": {"domain": [0, 250]}
    },
    "tooltip": [
      {"field": "Year", "type": "nominal", "title": "Year"},
      {"field": "AQI", "type": "quantitative", "title": "Average Daily AQI"}
    ]
  },
  "config": {}
}